{
  "name": "mask-sensitive-data",
  "version": "0.11.5",
  "description": "Mask sensitive data, eg. credit card numbers, phone numbers, emails, UUIDs, JWT tokens.",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "build:microbundler": "rm -rf build && microbundle --no-sourcemap",
    "lint": "eslint --ext .ts",
    "npm:publish": "npm run build && npm publish",
    "test": "jest",
    "test:coverage": "npm run test --  --coverage",
    "test:tdd": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akozlov75/mask-sensitive-data.git"
  },
  "keywords": [
    "credit card",
    "email",
    "jwt",
    "mask",
    "phone number",
    "sensitive data",
    "uuid"
  ],
  "author": "Alex Kozlov <akozlov75@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akozlov75/mask-sensitive-data/issues"
  },
  "homepage": "https://github.com/akozlov75/mask-sensitive-data#readme",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/json-stringify-safe": "5.0.3",
    "@types/node": "20.11.16",
    "eslint": "8.56.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "microbundle": "0.15.1",
    "node-notifier": "10.0.1",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "telejson": "7.2.0"
  }
}
